override_dh_auto_build:
	#Create settings file from default settings, installed duirng install.
	cp yubiadmin/default_settings.py yubiadmin.conf
	dh_auto_build


override_dh_installinit:
	dh_installinit --name yubiadmin

%:
	dh $@ --with python2 --buildsystem=python_distutils


